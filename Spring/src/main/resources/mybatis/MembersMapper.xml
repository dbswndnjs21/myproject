<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.MembersMapper.xml">

    <select id="select" resultType="test.vo.MembersVo">
        select * from members
    </select>

    <select id="selectOne" resultType="test.vo.MembersVo" parameterType="int">
        select * from members where num = #{num}
    </select>

    <insert id="insert" parameterType="test.vo.MembersVo">
        insert into members
        values (#{num}, #{name}, #{phone}, #{addr}, sysdate)
    </insert>

    <update id="update" parameterType="test.vo.MembersVo">
        update members
        set name = #{name} , phone = #{phone} , addr= #{addr}
        where num =#{num}
    </update>

    <delete id="delete" parameterType="int">
        delete from members
        where num = #{num}
    </delete>

</mapper>